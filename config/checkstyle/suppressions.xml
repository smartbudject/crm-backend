<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <!--    Repository-->
    <suppress checks="MissingJavadocMethod" files=".*Repository\.java"/>
    <suppress checks="MissingJavadocType" files=".*Repository\.java"/>


    <suppress checks="MissingJavadocMethod" files=".*Mapper\.java"/>
    <suppress checks="MissingJavadocType" files=".*Mapper\.java"/>
    <suppress checks="MissingJavadocMethod" files=".*Delegate\.java"/>

    <!--    Test-->
    <suppress checks="MissingJavadocMethod" files=".*Test\.java"/>
    <suppress checks="MissingJavadocType" files=".*Test\.java"/>
    <suppress checks="MultipleStringLiterals" files=".*Test\.java"/>
    <suppress checks="MethodName" files=".*Test\.java"/>

    <!--    Generated-->
    <suppress checks=".*" files="[/\\]target[/\\]"/>

</suppressions>

